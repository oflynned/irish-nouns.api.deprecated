{
  "name": "irish-nouns-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && babel src -d dist",
    "clean": "rimraf dist/*",
    "heroku-postbuild": "npm install && npm run build",
    "lint": "eslint --ignore-path .eslintignore . --fix",
    "lint:staged": "lint-staged",
    "migrate": "migrate-mongo up",
    "rollback": "migrate-mongo down",
    "serve": "NODE_ENV=production node dist/app.js",
    "sort-package-json": "sort-package-json",
    "start": "NODE_ENV=development nodemon src/app.js --exec babel-node",
    "test": "jest --runInBand --detectOpenHandles --forceExit"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.test.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "roots": [
      "test"
    ],
    "setupFilesAfterEnv": [
      "./test/jestSetup.js"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "acorn": "^6.3.0",
    "aws-sdk": "^2.568.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connection-string-parser": "^1.0.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "country-list": "^2.2.0",
    "currency-codes": "^1.5.1",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "express-fileupload": "^1.1.5",
    "faker": "^4.1.0",
    "googleapis": "^44.0.0",
    "hbs": "^4.0.6",
    "helmet": "^3.21.2",
    "http-errors": "~1.6.2",
    "joi": "^14.0.1",
    "joi-date-extensions": "^1.2.0",
    "lodash": "^4.17.15",
    "marpat": "^2.1.2",
    "migrate": "^1.6.2",
    "migrate-mongo": "^6.0.4",
    "mongodb": "latest",
    "monk": "^7.1.1",
    "morgan": "~1.9.0",
    "s3-zip": "^3.1.3",
    "sendgrid": "^5.2.3",
    "serve-favicon": "^2.5.0",
    "uuid": "^3.3.3",
    "uuid-validate": "0.0.3",
    "winston": "^3.2.1",
    "xml-stream": "^0.4.5",
    "ip-regex": "latest"
  },
  "devDependencies": {
    "babel-jest": "^25.0.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "factory-girl": "^5.0.4",
    "husky": "^1.3.1",
    "jest": "^25.0.0",
    "lint-staged": "^8.2.1",
    "mongo-in-memory": "0.0.5",
    "nodemon": "^1.19.4",
    "rimraf": "^2.7.1",
    "sinon": "^7.5.0",
    "sort-package-json": "^1.23.1",
    "timekeeper": "^2.2.0"
  }
}
